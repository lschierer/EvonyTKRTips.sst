{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "target": "ES2020",
    "module": "ESNext",
    "sourceMap": true,
    "declaration": true,
    "incremental": true,
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "moduleDetection": "force",
    "esModuleInterop": true,
    "noErrorTruncation": true,
    "strict": true,
    "noEmit": true
  }
}
