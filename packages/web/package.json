{
  "name": "@EvonyTKRTips/web",
  "description": "EvonyTKRTips Frontend",
  "version": "0.5.0",
  "engines": {
    "node": ">=16.16.0"
  },
  "browserslist": [
    "defaults"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "create": "pnpm hugo new",
    "prestart": "npm run clean",
    "start": "pnpm hugo server --bind=0.0.0.0 --disableFastRender --navigateToChanged --verbose ",
    "prebuild": "npm run clean",
    "build": "pnpm hugo --gc --minify",
    "build:preview": "npm run build -D -F",
    "clean": "shx rm -rf public resources",
    "clean:install": "shx rm -rf package-lock.json node_modules ",
    "lint": "npm run -s lint:scripts && npm run -s lint:styles && npm run -s lint:markdown",
    "lint:scripts": "eslint assets/js config functions",
    "lint:styles": "stylelint \"assets/scss/**/*.{css,sass,scss,sss,less}\"",
    "lint:markdown": "markdownlint-cli2 \"*.md\" \"content/**/*.md\"",
    "lint:markdown-fix": "markdownlint-cli2-fix \"*.md\" \"content/**/*.md\"",
    "server": "pnpm hugo server",
    "test": "npm run -s lint",
    "env": "env",
    "precheck": "npm version",
    "check": "pnpm hugo version",
    "copy:katex-fonts": "shx cp ./node_modules/katex/dist/fonts/* ./static/fonts/",
    "version": "auto-changelog -p && git add CHANGELOG.md"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.22.1",
    "@babel/preset-env": "^7.22.4",
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "@hyas/images": "^0.2.3",
    "auto-changelog": "^2.4.0",
    "autoprefixer": "^10.4.14",
    "bootstrap": "^5.3.0",
    "clipboard": "^2.0.11",
    "eslint": "^8.41.0",
    "exec-bin": "^1.0.0",
    "flexsearch": "^0.7.31",
    "highlight.js": "^11.8.0",
    "instant.page": "^5.2.0",
    "katex": "^0.16.7",
    "lazysizes": "^5.3.2",
    "markdownlint-cli2": "^0.4.0",
    "netlify-plugin-submit-sitemap": "^0.4.0",
    "node-fetch": "^3.3.1",
    "postcss": "^8.4.24",
    "postcss-cli": "^10.1.0",
    "purgecss-whitelister": "^2.4.0",
    "shx": "^0.3.4",
    "stylelint": "^14.16.1",
    "stylelint-config-standard-scss": "^4.0.0"
  },
  "dependencies": {
    "@adobe/coral-spectrum": "^4.15.18",
    "hugo-extended": "^0.112.3",
    "mathjax": "^3.2.2",
    "mermaid": "^10.2.2"
  }
}
